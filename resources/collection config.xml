<?xml version="1.0" encoding="UTF-8"?>

<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
               http://www.springframework.org/schema/context
               http://www.springframework.org/schema/context/spring-context.xsd">

    <bean name="employee1" class="org.example.collection.Employee">
        <property name="name" value="Rohan"/>

        <property name="phones">
            <list>
                <value>7987948810</value>
                <value>6262221708</value>
                <value>7240803224</value>
            </list>
        </property>

        <property name="addresses">
            <set>
                <value>Pune</value>
                <value>Delhi</value>
                <value>Indore</value>
            </set>

        </property>

        <property name="courses">
            <map>
                <entry key="Java" value="2Months"/>
                <entry key="Spring" value="1Month"/>
                <entry key="JavaScript" value="2Month"/>
            </map>
        </property>
    </bean>
</beans>