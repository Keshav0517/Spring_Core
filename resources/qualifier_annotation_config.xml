<?xml version="1.0" encoding="UTF-8"?>

<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
               http://www.springframework.org/schema/context
               http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>


    <bean name="address1" class="org.example.qualifier_annotation.Address">
        <property name="city" value="Pune"/>
        <property name="state" value="MH"/>
    </bean>


    <bean name="address2" class="org.example.qualifier_annotation.Address">
        <property name="city" value="Indore"/>
        <property name="state" value="MP"/>
    </bean>

    <bean name="student" class="org.example.qualifier_annotation.Student">
        <property name="id" value="101"/>
        <property name="name" value="Keshav"/>
    </bean>
</beans>